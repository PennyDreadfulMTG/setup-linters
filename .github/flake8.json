// https://github.com/actions/toolkit/blob/main/docs/problem-matchers.md
{
	"problemMatcher": [
		{
			"owner": "flake8",
			"severity": "warning", // fallback
			"pattern": [
				{
					"regexp": "^([^:]+):(\\d+):(?:(\\d+):) ([A-Z\\d]+) (.*)$",
					"file": 1,
					"line": 2,
					"column": 3,
					"code": 4,
					"message": 5
				}
			]
		}
	]
}
 